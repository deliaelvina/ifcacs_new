    "metro-react-native-babel-transformer": "^0.65.0",
    "metro-resolver": "^0.65.0",
    "metro-runtime": "^0.65.0"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  }
